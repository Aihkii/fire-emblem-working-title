{
  "name": "fire-emblem-heroes-stats",
  "version": "0.6.6",
  "main": "dist/index.js",
  "author": "Andrew Hyndman <ajhyndman@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run scrape-stats && npm run compile && npm run copy-types",
    "build:watch": "chokidar \"src/**/*.js\" --initial true --debounce 2000 --command \"npm run build\"",
    "compile": "rollup --config",
    "copy-types": "cp src/index.js.flow dist/index.js.flow",
    "prepublish": "npm run build",
    "scrape-stats": "babel-node build/index.js",
    "start": "npm run build:watch",
    "test": "eslint src/**/*.js build/**/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.16.1",
    "eslint-config-ajhyndman": "git+https://github.com/ajhyndman/eslint-config-ajhyndman#1.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.23.0"
  }
}
