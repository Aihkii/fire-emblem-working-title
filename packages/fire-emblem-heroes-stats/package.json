{
  "name": "fire-emblem-heroes-stats",
  "version": "0.6.6",
  "main": "dist/index.js",
  "author": "Andrew Hyndman <ajhyndman@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run compile && npm run copy-types && npm run scrape-stats",
    "compile": "babel src --out-dir dist",
    "copy-types": "cp src/index.js.flow dist/index.js.flow",
    "prepublish": "npm run build",
    "scrape-stats": "babel-node build/index.js",
    "start": "npm run build && npm run scrape-stats",
    "test": "eslint src/**/*.js build/**/*.js"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-ramda": "^1.1.6",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-ajhyndman": "git+https://github.com/ajhyndman/eslint-config-ajhyndman#1.0.2"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.23.0"
  }
}
